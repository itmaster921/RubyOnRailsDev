<tr id='admin-<%= admin.id %>'>
  <td>
    <a onclick="loadAdminModal(this);" data-admin-url="<%= edit_company_admin_path(company, admin) %>">
      <%= admin.full_name %>
    </a>
  </td>
  <td><%= admin.email %></td>
  <td><span class='label label-primary'><%= admin.level_to_s %></span></td>
  <td>
    <% if !admin.god? %>
      <%= link_to company_admin_path(company, admin), method: :delete, remote: true, class: 'del-admin' do  %>
        <button type="button" class="btn btn-primary ">
          <i class="fa fa-trash"></i>
        </button>
      <% end %>
    <% end %>
  </td>
</tr>
