<%= hidden_field_tag :authenticity_token, form_authenticity_token %>

<div class="row">
  <div class="col-md-12 ">
    <div class="form-group">
      <%= v.label :venue_name %>
      <%= v.text_field :venue_name, :class => 'form-control', :placeholder => true, autofocus: true, required: true %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <div class="form-group">
      <%= v.label :website %>
      <%= v.text_field :website, :class => 'form-control', :placeholder => true, autofocus: true %>
    </div>
  </div>
  <div class="col-md-6">
    <div class="form-group">
      <%= v.label :phone_number %>
      <%= v.text_field :phone_number, :class => 'form-control', :placeholder => true, autofocus: true %>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <div class="form-group">
      <%= v.label :description %>
      <%= v.text_area :description, rows: 5, :class => 'form-control', :placeholder => true, autofocus: true, required: true%>
    </div>
  </div>
</div>

<div class="row">
  <div class='col-md-4'>
    <div class="form-group">
      <%= v.label :street %>
      <%= v.text_field :street, :class => 'form-control', :placeholder => true, autofocus: true %>
    </div>
  </div>
  <div class='col-md-4'>
    <div class="form-group">
      <%= v.label :zip %>
      <%= v.text_field :zip, :class => 'form-control', :placeholder => true, autofocus: true %>
    </div>
  </div>
  <div class='col-md-4'>
    <div class="form-group">
      <%= v.label :city %>
      <%= v.text_field :city, :class => 'form-control', :placeholder => true, autofocus: true %>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <div class="form-group">
      <%= v.label :parking_info %>
      <%= v.text_area :parking_info, rows: 3, :class => 'form-control', :placeholder => true, autofocus: true %>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <div class="form-group">
      <%= v.label :transit_info %>
      <%= v.text_area :transit_info, rows: 3, :class => 'form-control', :placeholder => true, autofocus: true %>
    </div>
  </div>
</div>

<div class='row'>
  <div class="col-md-4">
    <div class="form-group">
      <%= v.label :booking_ahead_limit %>
      <%= v.text_field :booking_ahead_limit, :class => 'form-control', :placeholder => true, autofocus: true %>
      <span class='help-block m-b-none'>
        Number of days a user can book ahead.
      </span>
    </div>
  </div>
</div>

