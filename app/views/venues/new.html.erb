<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2><%= t('.header')%></h2>
        <ol class="breadcrumb">
            <li>
                <a href="#"><%= current_admin.company.company_legal_name %></a>
            </li>
            <li>
              <strong> <%= t('.title') %> </strong>
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12 white-bg">
      <div class="ibox float-e-margins">
        <div class="ibox-content p-md">
          <%= form_for @venue, :html => { multipart: true, class: "dropzone", id: "dropzone-area" } do |v| %>
            <%= render partial: 'form', locals: {v: v} %>

            <div class="row">
              <br>
              <div class="actions col-md-4">
                <button class="btn btn-primary ladda-button" type='submit' data-style="expand-right">
                  <%= t('.create_button') %>
                </button>
              </div>
            </div>
          <% end %>

          <br> <br><br>
        </div>
      </div>
    </div>
  </div>
</div>
